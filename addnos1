#include<stdio.h>
void getinput(int,int);
int add(int,int);
void output(int);
int main()
{
void getinput;
int num1,num2,res;
printf("Enter the two numbers\n");
scanf("%d%d",&num1, &num2);
res=add(num1,num2);
return(0);
}
int add(int num1,int num2);
{
res=num1+num2;
return(res);
}
void output()
{
printf("The sum of the two nos.:\n",res);
}
