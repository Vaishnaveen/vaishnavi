#include<stdio.h>
int getinput(int *a,int *b)
{
    printf("Enter the values of a and b\n");
    scanf("%d%d",a,b);
    return *a,*b;
}
void add(int *a,int *b);
{
    int res=*a+*b;
    return;
}
void output(int res)
{
     printf("The sum of the two nos.:\n",res);
}
int main()
{
    int a,b,res;
    a,b=input(&a,&b);
    add(a,b,&res);
    ouput(res);
}
